<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:WPFNode.Controls"
    xmlns:media="clr-namespace:System.Windows.Media;assembly=PresentationCore"
    xmlns:converters="clr-namespace:WPFNode.Converters"
    x:Class="WPFNode.Themes.Generic">

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

    <Style TargetType="{x:Type local:NodeControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:NodeControl}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="3"
                            Effect="{TemplateBinding Effect}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            
                            <!-- 헤더 -->
                            <Border Grid.Row="0" 
                                    Background="#FF2D2D30" 
                                    Padding="8,4">
                                <TextBlock Text="{Binding Model.Name}"
                                         Foreground="White"
                                         FontWeight="Bold"/>
                            </Border>

                            <!-- 포트 영역 -->
                            <Grid Grid.Row="1" Margin="4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <!-- 입력 포트 -->
                                <ItemsControl Grid.Column="0"
                                            x:Name="InputPortsPanel"
                                            ItemsSource="{Binding InputPorts}"
                                            Margin="0,0,4,0">
                                    <ItemsControl.ItemTemplate>
                                        <DataTemplate>
                                            <local:InputPortControl DataContext="{Binding}"
                                                                  Visibility="{Binding IsVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                        </DataTemplate>
                                    </ItemsControl.ItemTemplate>
                                </ItemsControl>

                                <!-- 노드 내용 -->
                                <ContentPresenter Grid.Column="1"
                                                Content="{TemplateBinding Content}"/>

                                <!-- 출력 포트 -->
                                <ItemsControl Grid.Column="2"
                                            x:Name="OutputPortsPanel"
                                            ItemsSource="{Binding OutputPorts}"
                                            Margin="4,0,0,0">
                                    <ItemsControl.ItemTemplate>
                                        <DataTemplate>
                                            <local:OutputPortControl DataContext="{Binding}"
                                                                   Visibility="{Binding IsVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                        </DataTemplate>
                                    </ItemsControl.ItemTemplate>
                                </ItemsControl>
                            </Grid>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <DataTrigger Binding="{Binding IsSelected}" Value="True">
                            <Setter Property="BorderBrush" Value="#FF007ACC"/>
                            <Setter Property="BorderThickness" Value="2"/>
                            <Setter Property="Effect">
                                <Setter.Value>
                                    <DropShadowEffect Color="#FF007ACC" 
                                                    ShadowDepth="0" 
                                                    Opacity="0.5"/>
                                </Setter.Value>
                            </Setter>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:PortControl}">
        <Setter Property="MinWidth" Value="50"/>
        <Setter Property="Height" Value="24"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Visibility" Value="{Binding IsVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:PortControl}">
                    <Grid Background="{TemplateBinding Background}"
                          Visibility="{Binding IsVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        
                        <!-- 연결점 -->
                        <Ellipse x:Name="PART_ConnectionPoint"
                                Width="12" Height="12"
                                Fill="#FF404040"
                                Stroke="#FF606060"
                                StrokeThickness="1"
                                Margin="0,6,0,6"/>
                                
                        <!-- 포트 이름 -->
                        <TextBlock x:Name="PART_PortName"
                                 Grid.Column="1"
                                 Text="{Binding Name}"
                                 VerticalAlignment="Center"
                                 Margin="4,0,0,0"
                                 Foreground="Black"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsInput" Value="True">
                            <Setter TargetName="PART_ConnectionPoint" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_ConnectionPoint" Property="HorizontalAlignment" Value="Left"/>
                            <Setter TargetName="PART_PortName" Property="Grid.Column" Value="1"/>
                            <Setter TargetName="PART_PortName" Property="TextAlignment" Value="Left"/>
                            <Setter TargetName="PART_PortName" Property="Margin" Value="4,0,0,0"/>
                        </Trigger>
                        <Trigger Property="IsInput" Value="False">
                            <Setter TargetName="PART_ConnectionPoint" Property="Grid.Column" Value="1"/>
                            <Setter TargetName="PART_ConnectionPoint" Property="HorizontalAlignment" Value="Right"/>
                            <Setter TargetName="PART_PortName" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_PortName" Property="TextAlignment" Value="Right"/>
                            <Setter TargetName="PART_PortName" Property="Margin" Value="0,0,4,0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:InputPortControl}" BasedOn="{StaticResource {x:Type local:PortControl}}"/>

    <Style x:Key="PropertyGridInputPortStyle" TargetType="{x:Type local:InputPortControl}" BasedOn="{StaticResource {x:Type local:PortControl}}">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="Visibility" Value="{Binding IsVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:PortControl}">
                    <Grid Background="{TemplateBinding Background}"
                          Visibility="{Binding IsVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <Ellipse x:Name="PART_ConnectionPoint"
                                Width="12" Height="12"
                                Fill="#FF404040"
                                Stroke="#FF606060"
                                StrokeThickness="1">
                            <Ellipse.Style>
                                <Style TargetType="Ellipse">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsConnected}" Value="True">
                                            <Setter Property="Fill" Value="#FF00AA00"/>
                                            <Setter Property="Stroke" Value="#FF00FF00"/>
                                        </DataTrigger>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Fill" Value="#FF606060"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Ellipse.Style>
                        </Ellipse>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:OutputPortControl}" BasedOn="{StaticResource {x:Type local:PortControl}}"/>

    <Style TargetType="{x:Type local:ConnectionControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:ConnectionControl}">
                    <Canvas>
                        <!-- 메인 연결선 -->
                        <Path x:Name="PART_Path"
                              Stroke="{TemplateBinding Foreground}"
                              StrokeThickness="2">
                            <Path.Data>
                                <PathGeometry>
                                    <PathGeometry.Figures>
                                        <PathFigureCollection>
                                            <PathFigure x:Name="PART_PathFigure">
                                                <PathFigure.Segments>
                                                    <PathSegmentCollection>
                                                        <BezierSegment x:Name="PART_BezierSegment"
                                                                      Point1="0,0" Point2="0,0" Point3="0,0"/>
                                                    </PathSegmentCollection>
                                                </PathFigure.Segments>
                                            </PathFigure>
                                        </PathFigureCollection>
                                    </PathGeometry.Figures>
                                </PathGeometry>
                            </Path.Data>
                        </Path>
                        
                        <!-- 화살표 -->
                        <Path x:Name="PART_Arrow"
                              Fill="{TemplateBinding Foreground}"
                              Stroke="{TemplateBinding Foreground}"
                              StrokeThickness="2">
                            <Path.Data>
                                <PathGeometry>
                                    <PathFigure StartPoint="0,0">
                                        <LineSegment Point="0,-6"/>
                                        <LineSegment Point="8,0"/>
                                        <LineSegment Point="0,6"/>
                                        <LineSegment Point="0,0"/>
                                    </PathFigure>
                                </PathGeometry>
                            </Path.Data>
                        </Path>
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:NodeGroupControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:NodeGroupControl}">
                    <Border Background="{Binding ViewModel.Color, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="3">
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:NodeCanvasControl}">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:NodeCanvasControl}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="250"/>
                            </Grid.ColumnDefinitions>

                            <!-- 캔버스 영역 -->
                            <ScrollViewer Grid.Column="0"
                                        x:Name="PART_ScrollViewer"
                                        HorizontalScrollBarVisibility="Auto"
                                        VerticalScrollBarVisibility="Auto">
                                <Canvas x:Name="PART_Canvas"
                                        Background="Transparent"
                                        ClipToBounds="True"
                                        Width="4000" Height="4000">
                                    <Canvas.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform ScaleX="{Binding Scale}"
                                                          ScaleY="{Binding Scale}"/>
                                        </TransformGroup>
                                    </Canvas.RenderTransform>

                                    <!-- 연결선 -->
                                    <ItemsControl ItemsSource="{Binding Connections}">
                                        <ItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <Canvas IsItemsHost="True" ZIndex="1"/>
                                            </ItemsPanelTemplate>
                                        </ItemsControl.ItemsPanel>
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <local:ConnectionControl DataContext="{Binding}"/>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>

                                    <!-- 노드 -->
                                    <ItemsControl ItemsSource="{Binding Nodes}">
                                        <ItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <Canvas IsItemsHost="True" ZIndex="2"/>
                                            </ItemsPanelTemplate>
                                        </ItemsControl.ItemsPanel>
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <local:NodeControl DataContext="{Binding}"/>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                        <ItemsControl.ItemContainerStyle>
                                            <Style TargetType="ContentPresenter">
                                                <Style.Resources>
                                                    <converters:CenterOffsetConverter x:Key="LocalCenterOffsetConverter"/>
                                                </Style.Resources>
                                                <Setter Property="Canvas.Left">
                                                    <Setter.Value>
                                                        <MultiBinding Converter="{StaticResource LocalCenterOffsetConverter}">
                                                            <Binding Path="Model.X"/>
                                                            <Binding ElementName="PART_Canvas" Path="Width"/>
                                                        </MultiBinding>
                                                    </Setter.Value>
                                                </Setter>
                                                <Setter Property="Canvas.Top">
                                                    <Setter.Value>
                                                        <MultiBinding Converter="{StaticResource LocalCenterOffsetConverter}">
                                                            <Binding Path="Model.Y"/>
                                                            <Binding ElementName="PART_Canvas" Path="Height"/>
                                                        </MultiBinding>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </ItemsControl.ItemContainerStyle>
                                    </ItemsControl>
                                </Canvas>
                            </ScrollViewer>

                            <!-- 속성 그리드 -->
                            <local:PropertyGrid Grid.Column="1"
                                              SelectedNode="{Binding SelectedNode}"
                                              CanvasViewModel="{Binding Path=.}"
                                              Background="White"
                                              BorderBrush="Gray"
                                              BorderThickness="0,0,0,1"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:SearchPanel}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:SearchPanel}">
                    <Grid>
                        <TextBox x:Name="PART_SearchBox"
                                 Padding="4"
                                 BorderThickness="1"
                                 BorderBrush="{TemplateBinding BorderBrush}"
                                 Background="{TemplateBinding Background}"
                                 Text="검색..."/>
                        
                        <Popup x:Name="PART_Popup"
                               PlacementTarget="{Binding ElementName=PART_SearchBox}"
                               Placement="Bottom"
                               Width="{Binding ActualWidth, ElementName=PART_SearchBox}"
                               MaxHeight="400"
                               AllowsTransparency="True">
                            <Border Background="White"
                                    BorderBrush="Gray"
                                    BorderThickness="1"
                                    CornerRadius="3"
                                    Padding="1">
                                <ListBox x:Name="PART_ResultList"
                                         Background="Transparent"
                                         BorderThickness="0">
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid Margin="4">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="Auto"/>
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>

                                                <TextBlock Text="{Binding Name}"
                                                         FontWeight="Bold"/>
                                                <TextBlock Grid.Column="1"
                                                         Text="{Binding Category}"
                                                         Foreground="Gray"
                                                         Margin="8,0,0,0"/>
                                                <TextBlock Grid.Row="1"
                                                         Grid.ColumnSpan="2"
                                                         Text="{Binding Description}"
                                                         TextWrapping="Wrap"
                                                         Foreground="Gray"
                                                         FontSize="11"
                                                         Margin="0,2,0,0"/>
                                            </Grid>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>
                            </Border>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- 색상 변환기 -->
    <converters:ColorToBrushConverter x:Key="ColorToBrushConverter"/>

    <!-- Value Converters -->
    <converters:CenterOffsetConverter x:Key="CenterOffsetConverter"/>
    <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>

    <!-- PropertyGrid 관련 리소스 -->
    <!-- 텍스트 입력 -->
    <DataTemplate x:Key="TextBoxTemplate">
        <TextBox Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                 MinWidth="60"
                 Margin="2"/>
    </DataTemplate>

    <!-- 여러 줄 텍스트 입력 -->
    <DataTemplate x:Key="MultilineTextTemplate">
        <TextBox Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                 TextWrapping="Wrap"
                 AcceptsReturn="True"
                 MinHeight="60"
                 VerticalScrollBarVisibility="Auto"
                 Margin="2"/>
    </DataTemplate>

    <!-- 숫자 입력 -->
    <DataTemplate x:Key="NumberBoxTemplate">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <TextBox Grid.Column="0" 
                    Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                    MinWidth="60"
                    Margin="2"
                    IsEnabled="{Binding IsConnected, Converter={StaticResource InverseBooleanConverter}}"/>
            <Button Grid.Column="1"
                    Content="연결"
                    Margin="2,2,2,2"
                    Padding="4,0"
                    Visibility="{Binding CanConnectToPort, Mode=TwoWay, Converter={StaticResource BooleanToVisibilityConverter}}"
                    Command="{Binding ConnectToPortCommand}"/>
        </Grid>
    </DataTemplate>

    <!-- 체크박스 -->
    <DataTemplate x:Key="CheckBoxTemplate">
        <CheckBox IsChecked="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                 VerticalAlignment="Center"
                 Margin="2"/>
    </DataTemplate>

    <!-- 색상 선택 -->
    <DataTemplate x:Key="ColorPickerTemplate">
        <ComboBox SelectedValue="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                 MinWidth="60"
                 Margin="2">
            <ComboBox.ItemsSource>
                <x:Array Type="media:Color">
                    <media:Color>White</media:Color>
                    <media:Color>Black</media:Color>
                    <media:Color>Red</media:Color>
                    <media:Color>Green</media:Color>
                    <media:Color>Blue</media:Color>
                    <media:Color>Yellow</media:Color>
                    <media:Color>Orange</media:Color>
                    <media:Color>Purple</media:Color>
                </x:Array>
            </ComboBox.ItemsSource>
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Width="16" Height="16" Margin="0,0,4,0">
                            <Rectangle.Fill>
                                <SolidColorBrush Color="{Binding}"/>
                            </Rectangle.Fill>
                        </Rectangle>
                        <TextBlock Text="{Binding}"/>
                    </StackPanel>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
    </DataTemplate>

    <!-- PropertyGrid 스타일 -->
    <Style TargetType="{x:Type local:PropertyGrid}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:PropertyGrid}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <!-- 검색 박스 -->
                        <TextBox Grid.Row="0" 
                                Text="{Binding SearchText, RelativeSource={RelativeSource TemplatedParent}, UpdateSourceTrigger=PropertyChanged}"
                                Margin="2"
                                Padding="2"/>

                        <!-- 속성 목록 -->
                        <DataGrid Grid.Row="1"
                                ItemsSource="{Binding FilteredProperties, RelativeSource={RelativeSource TemplatedParent}}"
                                AutoGenerateColumns="False"
                                CanUserAddRows="False"
                                CanUserDeleteRows="False"
                                CanUserReorderColumns="False"
                                HeadersVisibility="Column"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="속성" 
                                                  Binding="{Binding DisplayName}" 
                                                  Width="120"
                                                  IsReadOnly="True"/>
                                <DataGridTemplateColumn Header="값" Width="*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                                
                                                <ContentPresenter Grid.Column="0"
                                                                Content="{Binding}"
                                                                ContentTemplate="{Binding ControlTemplate}"/>
                                                
                                                <ToggleButton Grid.Column="1"
                                                            Width="16" Height="16"
                                                            Margin="4,0,0,0"
                                                            IsChecked="{Binding CanConnectToPort, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                            ToolTip="포트로 사용">
                                                    <ToggleButton.Style>
                                                        <Style TargetType="ToggleButton">
                                                            <Setter Property="Template">
                                                                <Setter.Value>
                                                                    <ControlTemplate TargetType="ToggleButton">
                                                                        <Border x:Name="border"
                                                                                BorderThickness="1"
                                                                                BorderBrush="#FF606060"
                                                                                Background="Transparent">
                                                                            <Path x:Name="icon"
                                                                                  Width="10" Height="10"
                                                                                  Stretch="Uniform"
                                                                                  Fill="#FF606060"
                                                                                  Data="M12,3C7.58,3 4,4.79 4,7C4,9.21 7.58,11 12,11C16.42,11 20,9.21 20,7C20,4.79 16.42,3 12,3M4,9V12C4,14.21 7.58,16 12,16C16.42,16 20,14.21 20,12V9C20,11.21 16.42,13 12,13C7.58,13 4,11.21 4,9M4,14V17C4,19.21 7.58,21 12,21C16.42,21 20,19.21 20,17V14C20,16.21 16.42,18 12,18C7.58,18 4,16.21 4,14Z"/>
                                                                        </Border>
                                                                        <ControlTemplate.Triggers>
                                                                            <Trigger Property="IsChecked" Value="True">
                                                                                <Setter TargetName="border" Property="Background" Value="#FF404040"/>
                                                                                <Setter TargetName="icon" Property="Fill" Value="#FFFFFFFF"/>
                                                                            </Trigger>
                                                                        </ControlTemplate.Triggers>
                                                                    </ControlTemplate>
                                                                </Setter.Value>
                                                            </Setter>
                                                        </Style>
                                                    </ToggleButton.Style>
                                                </ToggleButton>
                                            </Grid>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary> 
